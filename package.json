{
  "name": "@bippo-libs/msal-react-provider",
  "version": "0.0.8",
  "description": "Simplified msal provider in react projects suitable for common use cases",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:types && npm run build:package.json",
    "build:js": "node esbuild.mjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:package.json": "cp package.json ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "dependencies": {
    "@azure/msal-browser": "^4.13.0",
    "@azure/msal-react": "^3.0.12",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "prettier": "^3.5.3"
  }
}
